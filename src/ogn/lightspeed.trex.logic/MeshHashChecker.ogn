{
  "lightspeed.trex.logic.MeshHashChecker": {
    "description": ["Detects if a specific mesh is currently being drawn in the scene.\n\nChecks if a specific mesh hash was processed in the current frame."],
    "version": 1,
    "uiName": "Mesh Hash Checker",
    "language": "python",
    "categoryDefinitions": "config/CategoryDefinition.json",
    "categories": "Sense",
    "inputs": {
      "meshHash": {
        "description": ["The mesh hash to check for usage in the current frame."],
        "type": "token",
        "default": "0x0",
        "uiName": "Mesh Hash"
      }
    },
    "outputs": {
      "isUsed": {
        "description": ["True if the mesh hash was used in the current frame."],
        "type": "bool",
        "default": false,
        "uiName": "Is Used"
      },
      "usageCount": {
        "description": ["Number of times the mesh hash was used in the current frame."],
        "type": "float",
        "default": 0.000000,
        "uiName": "Usage Count"
      }
    }
  }
}
